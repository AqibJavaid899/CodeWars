def song_decoder(song):
    alpha = song.split('WUB');
    alpha1 = []
    res1 = ''
    res2 = ''
    for i in alpha:
        if i == '':
            continue;
        else:
            alpha1.append(i)
            res1 += i;
            res1 += " ";
    k=0;
    while k < (len(res1)-1):
        res2 += res1[k];
        k+=1
    return (res2)
